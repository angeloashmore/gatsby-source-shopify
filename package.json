{
  "name": "gatsby-source-shopify2",
  "version": "2.0.5",
  "description": "Gatsby source plugin for building websites using Shopfiy as a data source. This is based on the original plugin along with some bug fixes for gatsby v2 and downloading images for products.",
  "scripts": {
    "build": "babel src --out-dir .",
    "format": "prettier --write 'src/**/*.js' README.md",
    "prepublish": "npm run build",
    "watch": "npm run build -- --watch"
  },
  "homepage": "https://github.com/nikhilag/gatsby-source-shopify",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikhilag/gatsby-source-shopify.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "shopify"
  ],
  "author": "Nikhil Agarwal (Original by: Angelo Ashmore)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "prettier": "^1.13.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-source-filesystem": "^1.5.39",
    "graphql-request": "^1.6.0",
    "lodash": "^4.17.4",
    "p-iteration": "^1.1.7",
    "prettyjson": "^1.2.1"
  }
}
